{
  "name": "strudel-cli",
  "version": "1.0.0",
  "description": "CLI for playing Strudel live-coding music from the terminal",
  "type": "module",
  "bin": {
    "strudel": "./bin/strudel.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "@strudel/core": "^1.2.0",
    "@strudel/mini": "^1.2.0",
    "@strudel/soundfonts": "^1.3.0",
    "@strudel/tonal": "^1.2.0",
    "@strudel/transpiler": "^1.2.0",
    "@strudel/webaudio": "^1.3.0",
    "commander": "^12.0.0",
    "node-web-audio-api": "^1.0.0",
    "superdough": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
